x = 1;
cont = 0;
while(x < 10000) do{
	[WALK East];
	if( cont == 34 ) then{
		[WALK Southwest];
		cont = 0;
	}
	[TAKE East];
	[TAKE Southeast];
	[TAKE Northeast];
	decision = [ASK Is_in_enemy_base];
	print "Minha decisao eh ", decision;
	println;
	if( decision == 1) then{
		[DROP East];
	}
	x = x+1;
	cont = cont + 1;	
}
